// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Visit {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  date String
  host String
}

model Category {
  id  Int @id @default(autoincrement())
  createdAt DateTime  @default(now())
  name String  @unique
}

model Tag {
  id  Int @id @default(autoincrement())
  createdAt DateTime  @default(now())
  name String  @unique
}